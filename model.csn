{
  "definitions": {
    "SalesService": {
      "kind": "service",
      "@path": "/odata/v4/sales"
    },
    "SalesService.SalesOrders": {
      "kind": "entity",
      "projection": {
        "from": {
          "ref": [
            "sales.ops.SalesOrder"
          ]
        }
      },
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "orderNo": {
          "type": "cds.String",
          "length": 50
        },
        "parentSO": {
          "type": "cds.Association",
          "target": "SalesService.SalesOrders",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        },
        "customer": {
          "type": "cds.Association",
          "target": "SalesService.Customers",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        },
        "expectedShipDate": {
          "type": "cds.Date"
        },
        "plant": {
          "type": "cds.String",
          "length": 100
        },
        "status": {
          "type": "cds.String",
          "length": 30
        },
        "priority": {
          "type": "cds.Integer",
          "default": {
            "val": 0
          }
        },
        "currentStage": {
          "type": "cds.String",
          "length": 30
        },
        "lastActivity": {
          "type": "cds.DateTime"
        },
        "remarks": {
          "type": "cds.String",
          "length": 500
        },
        "items": {
          "type": "cds.Composition",
          "cardinality": {
            "max": "*"
          },
          "target": "SalesService.SalesOrderItems",
          "on": [
            {
              "ref": [
                "items",
                "parentSO"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        },
        "stages": {
          "type": "cds.Composition",
          "cardinality": {
            "max": "*"
          },
          "target": "SalesService.OrderStages",
          "on": [
            {
              "ref": [
                "stages",
                "order"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        }
      }
    },
